				<key>script</key>
				<string>on run argv
    set portNumber to item 1 of argv
    set killCommand to "lsof -ti:" &amp; portNumber &amp; " | xargs kill -9"

    -- Copy to clipboard
    set the clipboard to killCommand

    -- Paste into active document
    tell application "System Events"
        keystroke "v" using command down
    end tell

    -- Show notification
    display notification "Command pasted: " &amp; killCommand with title "Port Killer"

    return killCommand
end run</string>
